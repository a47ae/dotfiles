URxvt.geometry: 180x60

Xcursor.theme: DMZ-White

URxvt.font:                xft:Iosevka:pixelsize=15
URxvt.boldFont:            xft:Iosevka:pixelsize=15:bold
URxvt.italicFont:          xft:Iosevka:pixelsize=15:italic
URxvt.boldItalicFont:      xft:Iosevka:pixelsize=15:bold:italic
URxvt.letterSpace:         -1

URxvt.transparent:   false
URxvt.tintColor:     white
URxvt.shading:       20 

!black
