!magenta
URxvt*color5:              #f775c5
!cyan
URxvt*color6:              #42bfe1
!white
URxvt*color7:              #d1d4e0
!bright-black
URxvt*color8:              #686868
!bright-red
URxvt*color9:              #e99377
!bright-green
URxvt*color10:             #8fc36c
!bright-yellow
URxvt*color11:             #fffc67
!bright-blue
URxvt*color12:             #6ea3f6
!bright-magenta
URxvt*color13:             #fd9acc
!bright-cyan
